<%- include('../partials/head') %>

<h2>Login</h2>

<form method="POST" action="/auth/login">
    <div>
        <label for="username">Username: </label>
        <input 
          type="text" 
          placeholder="username" 
          name="username">
    </div>
    <div>
        <label>Password: </label>
        <input 
          type="password" 
          placeholder="Password" 
          name="password">
    </div>
    <div>
        <input type='submit' value='Log In!'>
    </div>
</form>

<%- include('../partials/foot') %>